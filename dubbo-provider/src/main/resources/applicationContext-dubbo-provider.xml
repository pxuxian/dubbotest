<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
	http://www.springframework.org/schema/beans/spring-beans.xsd  
	http://code.alibabatech.com/schema/dubbo  
	http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<dubbo:application name="my_provider" />

	<dubbo:registry address="multicast://224.5.6.7:1234" />

	<!-- <dubbo:registry address="zookeeper://10.10.73.51:2181" /> -->

	<dubbo:protocol name="dubbo" port="20880" />

	<bean id="demoService" class="com.youku.dubbo.provider.DemoServiceImpl" />
	<dubbo:service interface="com.youku.dubbo.provider.DemoService" ref="demoService" />

</beans>